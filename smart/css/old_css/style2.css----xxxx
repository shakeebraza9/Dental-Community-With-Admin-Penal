.login_page {
    position: fixed;
    width: 100%;
    text-align: center;
    height: 100%;
    z-index: 999;
    background: #fff;
    background-image: url(../webImages/login.png);
    background-size: contain;
    background-position: center;
    background-repeat: no-repeat;
}

.login_page_main {
    position: relative;
    width: calc(100% - 40px);
    max-width: 540px;
    min-height: 540px;
    margin: 0 auto;
    display: block;
    background-color: rgba(255, 255, 255, 0.9);
    border-radius: 50%;
    box-shadow: 0px 0px 8px #00000082;
    top: 50%;
    transform: translateY(-50%);
}

.login-center {
    position: absolute;
    width: 100%;
    left: 0;
    padding: 90px 50px;
    top: 50%;
    transform: translateY(-50%);
}

.content_logo {
    position: relative;
    width: 100%;
    text-align: center;
}

.content_logo a {
    width: 100%;
    display: block;
}

.content_logo a img {
    width: 100px;
    display: inline-block;
    vertical-align: middle;
}

.content_logo a h1 {
    position: relative;
    font-size: 28px;
    color: #01abbf;
    white-space: nowrap;
    transition: 0.3s;
    display: inline-block;
    vertical-align: middle;
    text-transform: uppercase;
    letter-spacing: 2px;
}

.content_logo a h1 span {
    font-size: 17px;
    display: block;
    letter-spacing: 1px;
}

.content_logo h4 {
    color: #666;
    font-size: 12px;
    margin-top: 6px;
}

.content_logo h2 {
    color: #666;
    font-size: 14px;
    margin-top: 20px;
}

.content_logo a:hover h1 {
    color: #666;
}

.login_main .submit_class_login {
    width: 100%;
    border: 1px solid #01abbf;
    padding: 5px;
    margin-top: 10px;
    border-radius: 5px;
    background-color: #01abbf;
    color: #fff;
    cursor: pointer;
    transition: 0.3s;
}

.submit_class_login:hover {
    background-color: #666 !important;
    border-color: #666;
}

.login_main .form-group:before {
    font-family: "Font Awesome 5 Free";
    font-weight: 900;
    position: absolute;
    left: 5px;
    top: 5px;
    color: #01abbf;
    font-size: 18px;
    z-index: 1;
}

.logo_animation {
    position: relative;
    width: 100%;
    animation: spin 15s linear infinite;
    max-width: 262px;
    margin: 0 auto;
}

.content_logo img {
    width: 100%;
}

.logo_text {
    position: relative;
    width: 100%;
    max-width: 282px;
    margin: 0 auto;
    margin-top: 15px;
}

.login_main {
    position: relative;
    width: 100%;
    padding: 20px 50px;
}

.login_main form {
    position: relative;
    width: 100%;
    text-align: left;
}

.login_main .form-group {
    position: relative;
}

.login_main input[type="password"],
.login_main input[type="text"],
.login_main input[type="email"] {
    width: 100%;
    position: relative;
    border: none;
    border-bottom: 1px solid #c9c9c9;
    background-color: transparent !important;
    font-size: 14px;
    padding: 8px;
    padding-left: 30px;
}

.login_main input:-webkit-autofill,
.login_main input:-webkit-autofill:hover,
.login_main input:-webkit-autofill:focus {
    background-color: transparent !important;
}

.login_main .tooltip {
    position: relative;
    width: 100%;
    text-align: left;
    opacity: 1;
    margin: 10px 0;
    overflow: hidden;
    color: #01abbf;
    font-size: 12px;
    font-family: "montserratmedium";
}

.login_main .tooltip label {
    font-size: 12px;
    font-family: "montserratmedium";
}

.login_main .tooltip a {
    color: #01abbf;
    font-family: "montserratmedium";
    float: right;
    font-size: 12px;
}

.login_main .tooltip a:hover {
    color: #666;
}

.form-group.fa-key button {
    position: absolute;
    right: 0;
    background: #01abbf;
    border: transparent;
    color: #fff;
    height: 35px;
    bottom: 0;
    width: 35px;
    cursor: pointer;
}

.rst {
    display: block;
    margin-top: 15px;
    font-size: 14px;
    font-family: "montserratmedium";
    color: #484848;
}

.rst a {
    color: #0f5171;
}

.rst a:hover {
    color: #73a442;
}

.header_bottom {
    position: relative;
    width: 100%;
    background: #fff;
    box-shadow: 0px 5px 5px #ececec;
    z-index: 1;
    display: flow-root;
}

.header_bottom_right {
    position: relative;
    float: right;
}

.login_side {
    background: #01abbf;
    position: relative;
    transition: 0.5s;
    display: inline-block;
    vertical-align: middle;
}

.login_side a {
    display: block;
    padding: 14.5px 15px;
}

.login_side h6 {
    position: relative;
    width: 100%;
    color: #fff;
    font-size: 12px;
    font-family: "montserratlight";
    margin-bottom: 3px;
    transition: 0.5s;
}

.login_side h2 {
    position: relative;
    width: 100%;
    color: #fff;
    font-size: 16px;
    font-family: "montserratsemibold";
    transition: 0.5s;
}

.login_side:hover {
    background: #666;
}

.login_side:hover h6 {
    color: #ffffff;
}

.login_side:hover h2 {
    color: #ffffff;
}

.account_btn {
    position: relative;
    display: inline-block;
    vertical-align: middle;
    margin-left: 8px;
}

.account_btn ul {
    list-style: none;
    padding: 0px 0px;
    margin: 0px 0px;
}

.account_btn ul li {
    position: relative;
    display: block;
    transition: 0.5s;
}

.account_btn ul li a {
    font-size: 13px;
    color: #01abbf;
    font-family: "montserratlight";
    text-decoration: underline;
}

.account_btn ul li a span {
    margin-left: 8px;
    transition: 0.5s;
    display: inline-block;
    vertical-align: top;
}

.account_btn ul li:hover a {
    color: #666;
}

.account_btn ul li:hover span {
    filter: grayscale(100%);
}

.left_side {
    position: sticky;
    display: inline-block;
    vertical-align: top;
    width: 240px;
    padding-right: 50px;
    top: 0;
    z-index: 2;
}

.left_right_side:before {
    content: "";
    position: absolute;
    width: 190px;
    background-color: #01abbf;
    top: 0;
    height: 100%;
}

.col1_btnn {
    position: relative;
    width: 100%;
    background: #01abbf;
    text-align: center;
    border-radius: 5px;
    transition: 0.5s;
    height: 37px;
}

.col1_btn_practices {
    position: relative;
    width: 100%;
    background: #01abbf;
    text-align: center;
    border-radius: 5px;
    transition: 0.5s;
    height: 37px;
}

.col1_btn_practices a {
    display: block;
    font-family: "montserratmedium";
    font-size: 16px;
    color: #ffffff;
    padding: 6px 0px;
}

.col1_btnn a {
    display: block;
    font-family: "montserratmedium";
    font-size: 16px;
    color: #ffffff;
    padding: 6px 0px;
}

.col1_btn22 {
    width: 120px;
    margin-left: 20px;
}

.col1_btn33 {
    margin-left: 20px;
}

.col1_btn:hover {
    background: #4d4f5c;
}

.col1_h3 {
    position: relative;
    display: inline-block;
    vertical-align: middle;
    width: 84%;
}

.col1_btn2 {
    position: relative;
    width: 100%;
    text-align: center;
    background: #f05830;
    border-radius: 5px 5px;
    margin-top: 30px;
    transition: 0.5s;
}

.col1_btn2 a {
    display: block;
    padding: 12px 0px;
}

.col1_btn2 h3 {
    position: relative;
    width: 100%;
    color: #ffffff;
    font-size: 14px;
    font-family: "montserratmedium";
}

.col1_btn2 h2 {
    position: relative;
    width: 100%;
    color: #ffffff;
    font-size: 40px;
    font-family: "montserratbold";
}

.col1_btn2:hover {
    background: #72a344;
}

.col_news {
    position: relative;
    width: 100%;
    margin-top: 25px;
}

.col_news h3 span img {
    vertical-align: middle;
}

.col_news h3 {
    font-size: 24px;
}

.col_news ul {
    position: relative;
    width: 100%;
}

.col_news ul li {
    position: relative;
    width: 100%;
    padding: 10px 0px;
    margin: 0px 0px;
}

.col_news ul li a {
    display: block;
}

.col_news ul li h6 {
    position: relative;
    width: 100%;
    color: #989898;
    font-size: 11px;
    font-family: "montserratlight";
    text-shadow: 0px 0px 0px #989898;
}

.col_news ul li h5 {
    position: relative;
    width: 100%;
    color: #000000;
    text-shadow: 0px 0px 0px #000000;
    font-size: 14px;
    font-family: "montserratlight";
    margin: 5px 0px;
}

.col_news ul li h4 {
    position: relative;
    width: 100%;
    color: #0f5171;
    text-shadow: 0px 0px 0px #0f5171;
    font-size: 11px;
    font-family: "montserratlight";
    transition: 0.5s;
}

.col_news ul li a:hover h4 {
    color: #72a344;
}

.right_side {
    position: relative;
    width: calc(100% - 270px);
    display: inline-block;
    vertical-align: top;
    padding: 30px 0;
}

.right_side_top {
    position: relative;
    width: 100%;
    margin-bottom: 30px;
    overflow: hidden;
}

.right_side_top h3 span {
    font-size: 20px;
}

.right_side_top .col1_btnn {
    max-width: 340px;
    float: right;
}

.right_side_top .col1_btn_practices {
    max-width: 179px;
    float: right;
    margin: 0px 25px;
}

.col22 {
    position: relative;
    width: 100%;
    padding: 50px 80px;
    background: #fff;
    border-radius: 10px;
    margin: 20px 0 40px;
}

.col2_main {
    position: relative;
    width: 100%;
    display: flow-root;
}

.col2_main_left {
    position: relative;
    float: left;
    width: 46%;
    box-shadow: 0px 0px 8px #aaa;
    border-radius: 10px;
    overflow: hidden;
    min-height: 370px;
}

.col2_main_right {
    position: relative;
    float: right;
    width: 46%;
    box-shadow: 0px 0px 8px #aaa;
    border-radius: 10px;
    min-height: 370px;
}

.col33 {
    position: relative;
    width: 100%;
    margin-top: 40px;
    overflow: hidden;
}

.col33_left {
    position: relative;
    display: inline-block;
    vertical-align: top;
    float: left;
    width: 46%;
    box-shadow: 0px 0px 6px #999;
    border-radius: 10px;
}

.col33_left_main {
    position: relative;
    width: 100%;
    padding: 20px 20px;
    border-radius: 10px;
    background: #01abbf;
}

.col33_left_main h3 {
