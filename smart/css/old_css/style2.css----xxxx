/*Sticky Note*/


.jSticky-large{
    /*background:transparent url(../images/stickynote-large.png) no-repeat top left;*/
    width:225px;
    height:228px;
    position: absolute;
    z-index:999999;
    background: #fcdc83;
    -moz-box-shadow: 5px 5px 7px rgba(33,33,33,1);
    -webkit-box-shadow: 5px 5px 7px rgba(33,33,33,.7);
    box-shadow: 5px 5px 7px rgba(33,33,33,.7);
    -webkit-transform: rotate(-6deg);
    -o-transform: rotate(-6deg);
    -moz-transform: rotate(-6deg);
    -moz-transition: -moz-transform .15s linear;
    -o-transition: -o-transform .15s linear;
    -webkit-transition: -webkit-transform .15s linear;
    border-radius: 15px 0px 15px 0px;
}

.jSticky-large:hover{
    -webkit-transform: rotate(0deg);
    -o-transform: rotate(0deg);
    -moz-transform: rotate(0deg);
    -webkit-transform: scale(1.1);
    -o-transform: scale(1.1);
    -moz-transform: scale(1.1);
}

.jSticky-large:nth-child(2n){
    background: #ffccac;
    -webkit-transform: rotate(6deg);
    -o-transform: rotate(6deg);
    -moz-transform: rotate(6deg);
}

.jSticky-large:nth-child(2n):hover{
    -webkit-transform: rotate(0deg);
    -o-transform: rotate(0deg);
    -moz-transform: rotate(0deg);
    -webkit-transform: scale(1.1);
    -o-transform: scale(1.1);
    -moz-transform: scale(1.1);
}

.jSticky-medium{
    background:transparent url(../images/stickynote-medium.png) no-repeat top left;
    width:160px;
