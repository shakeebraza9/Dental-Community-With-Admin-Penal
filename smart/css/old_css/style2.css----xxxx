.piesite {
    position: relative;
    width: 1.2em;
    height: 1.2em;
    font-size: 10em;
    cursor: default;
    margin: 0 auto;
}

.piesite .percent {
    position: absolute;
    top: 1.3em;
    left: 0;
    width: 100%;
    font-size: 40px;
    text-align: center;
    z-index: 2;
    color: #fff;
}

.piesite .percent h5 {
    color: #ffffff;
    font-size: 50px;
    font-family: "montserratbold";
    width: 100%;
}

.piesite>#slice {
    position: absolute;
    width: 1.2em;
    height: 1.2em;
    clip: rect(0px, 1.2em, 1.2em, 0.7em);
}

.piesite>#slice.gt50 {
    clip: rect(auto, auto, auto, auto);
}

.piesite>#slice>.pie {
    position: absolute;
    border: 0.1em solid #444;
    width: 1em;
    height: 1em;
    clip: rect(0em, 0.5em, 1em, 0em);
    border-radius: 0.5em;
}

li.design .piesite>#slice>.pie {
    border-color: #fff;
    box-shadow: 5px 5px 8px inset rgba(0, 0, 0, 0.2);
}

li.usability .piesite>#slice>.pie {
    border-color: #8dc63f;
}

li.creativity .piesite>#slice>.pie {
    border-color: #8dc63f;
}

.piesite>#slice>.pie.fill {
    -webkit-transform: rotate(180deg) !important;
    transform: rotate(180deg) !important;
}

.piesite.fill>.percent {
    display: none;
}

.piesite:after {
    content: "";
    display: block;
    position: absolute;
    top: 0.1em;
    left: 0.1em;
    width: 1em;
    height: 1em;
    background: #01abbf;
    border-radius: 100%;
    border: 15px solid #fff;
    z-index: 1;
}

.piesite:before {
    content: "";
    display: block;
    position: absolute;
    width: 1em;
    height: 1em;
    border-radius: 0.5em;
    opacity: 0.5;
    z-index: 0;
}

.piesite:before {
    background: transparent;
}

.col2_main_right li.design .piesite .percent {
    top: 1.7em;
}

.col2_main_right li.design .piesite>#slice>.pie {
    width: 1.2em;
    height: 1.2em;
    clip: rect(0em, 0.7em, 1.2em, 0em);
    border-radius: 1em;
    border: 2px solid #01abbf;
}

.chartBarsHorizontal .chartBars .bars {
    flex-direction: column;
}

.box-piesite_txt {
