.canvas {
    width: calc(100% - 40px);
    display: block;
    height: 350px;
    position: relative;
    overflow: hidden;
    margin: auto;
    display: block;
}

.canvas:before {
    content: "";
    position: absolute;
    top: 0;
    left: 1px;
    right: 1px;
    height: 20px;
    background-color: #e5f3f2;
    z-index: 1;
}

.canvas:after {
    content: "";
    position: absolute;
    bottom: 0;
    left: 1px;
    right: 1px;
    height: 20px;
    background-color: #e5f3f2;
    z-index: 1;
}

.canvas video {
    width: calc(100% - 40px);
    position: absolute;
    left: 20px;
    top: 50%;
    transform: translateY(-50%) !important;
}

