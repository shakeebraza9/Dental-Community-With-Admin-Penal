       margin-bottom: 15px;
       padding-left: 8px;
       padding-right: 8px;
       max-height: 30px;
      /*overflow: hidden;*/
}
.rota #draggable div:hover, .rota #draggable div:focus,.ui-draggable {
      max-height: unset;
      overflow: visible;
}
.rota #draggable li:before {
    content:'';
    position:absolute;
    width:100%;
    height:100%;
    /*background:rgb(0 0 0 / 49%);*/
    background:rgb(0 0 0 / 30%);
    
}
.rota #draggable li.ho:before,.rota #draggable li.dyo:before,.rota #draggable li.sk:before {
    background:none;
    
}
.rota #draggable li p {
    z-index: 2;
}

.rota .rota-table td {
position: relative;
}

.rota .rota-table p {
    z-index: 2;
    position: relative;
}


.rota .rota-table td:before {
content: '';
    position: absolute;
    width: 100%;
    left: 0;
    top: 0;
    height: 100%;
    z-index: 1;
    background-color: rgb(0 0 0 / 20%);
}

.rota #draggable li {

    display: flex;
    flex-flow: column;
    justify-content: center;
    align-items: center;
    color: #fff;
    font-family: "montserratmedium";
    padding: 10px;
    border-radius: 5px;
    text-align: center;
    position: relative;
    height: 100%;
    overflow: hidden;
}

.rota #draggable li a {
    
    position: absolute;
    right: 1px;
    top: 1px;
    color: #ff0101;
    background-color: #fff;
    height: 25px;
    width: 25px;
    border-radius: 4px;
    line-height: 1.6;
    transition: 0.3s;
    z-index: 10;
}
.rota #draggable li .ahide{
    display: none;
}

.rota #draggable li:hover .ahide{
    display:block;

}
.rota #draggable li:hover a{
    background-color: #ff0101;
    color: #fff;
}

.rota #draggable li.addshift {
    background-color: #f9f9f9;
    border: 1px solid #cedad9;
}

.rota #draggable li.addshift button {
    margin: 0;
}

.rota #draggable li.ui-draggable-dragging a {
    display: none;
}

.rota #draggable li.ui-draggable-dragging.addshift {
    display: none;
}

