/* --------------------------Main Styling-------------------------------------*/

.myevents-form{
    min-height: 200px;
}
.myevents-form #loader{
    background: none;
}


.myevents-div-profile-detail .myevents-div {
    position: relative;
    margin: 0px auto;
    max-width: 100%;
    width: 100%;
    transform: scaleX(1) translateY(0);
    max-height: 100%;
    height: 100%;
    overflow-y: auto;
    z-index: 0;
}

.myevents-form_profile_detail {
    position: fixed;
    left: 0%;
    right: 0%;
    width: 90%;
    max-width: 1000px;
    margin: 0 auto;
    top: 50%;
    z-index: 999;
    padding: 20px 25px 25px;
    background: #fff;
    border-top: 5px solid #01abbf;
    transform: scaleX(0) translateY(-50%);
    transition: 0.5s;
    max-height: 90%;
    overflow-y: scroll;
}

.menu_icn {
    float: right;
    width: 40px;
    margin-top: 30px;
    margin-right: 10px;
    display: none;
}

.menu_icn a {
    display: block;
}

.menu_icn a img {
    width: 100%;
}

.index_content {
    background-color: #fff;
}

/*packages start*/

.packages {
    width: 100%;
    position: relative;
    padding: 100px 0;
}

.packages .packages_left {
    width: 49%;
    display: inline-block;
    vertical-align: middle;
    padding-right: 20px;
}

.packages .packages_left img {
    width: auto;
    max-width: 100%;
    margin: 0 auto;
}

.packages .packages_left h1 {
    color: #fff;
    font-size: 30px;
    font-family: "montserratlight";
    letter-spacing: -1px;
    margin-bottom: 20px;
}

.packages .packages_right {
    width: 49%;
    display: inline-block;
    vertical-align: middle;
}

.packages .packages_right h2 {
    color: #fff;
    font-size: 20px;
    letter-spacing: -1px;
    margin-bottom: 20px;
}

.packages .packages_right ul {
    list-style: none;
    margin: 0;
    padding: 0;
}

.packages .packages_right ul li {
    color: #ffffff;
    font-size: 16px;
    margin-bottom: 10px;
    position: relative;
    padding-left: 35px;
}

.packages .packages_right ul li:before {
    content: "";
    height: 25px;
    width: 25px;
    display: inline-block;
    vertical-align: middle;
    background-image: url(../webImages/tick.png);
    background-size: cover;
    background-repeat: no-repeat;
    position: absolute;
    top: 0;
    left: 0;
}

.packages_right a {
    color: #71a22f;
    white-space: nowrap;
    display: block;
    font-family: "montserratmedium";
    font-size: 16px;
    position: relative;
}

.packages_right a i {
    border-radius: 50%;
    height: 20px;
    width: 20px;
    border: 2px solid #71a22f;
    font-size: 10px;
    text-align: center;
    line-height: 1.6;
    margin-left: 10px;
    transition: 0.3s;
}

.packages_right a:hover {
    color: #fff;
}

/*packages end*/

.button_side {
    position: absolute;
    width: 200px;
    height: 45px;
    line-height: 45px;
    overflow: hidden;
    text-align: center;
    background: #71a22f;
    right: 0%;
    left: 0%;
    margin: 0 auto;
    top: 8%;
    z-index: 4;
    display: none;
}

.background_side {
     width: 100%;
  background: rgba(0, 0, 0, 0.6);
  height: 100%;
  z-index: 9;
  position: fixed;
  left: 0px;
  top: 0px;
  display: none;
}

.button_side a {
    display: block;
    color: #fff;
    font-size: 16px;
    position: relative;
    z-index: 1;
}

.button_side:before {
    content: "";
    position: absolute;
    left: 0%;
    top: 0%;
    width: 0%;
    height: 100%;
    background: #434343;
    transition: 0.7s;
}

.button_side:after {
    content: "";
    position: absolute;
    right: 0%;
    top: 0%;
    width: 0%;
    height: 100%;
    background: #434343;
    transition: 0.7s;
}

.button_side:hover:before {
    width: 100%;
}

.button_side:hover:after {
    width: 100%;
}

td,
th {
    padding: 5px;
}

ul #slider {
    max-height: 200px !important;
}

.main {
    transition: 0.7s;
}

.main:hover {
    background: #50e56a4d;
}

.main:nth-child(1):hover {
    background: #01aee84d;
}

.main:nth-child(2):hover {
    background: #d299024d;
}

.main:nth-child(3):hover {
    background: #f1008d4d;
}

.main:nth-child(4):hover {
    background: #a4cc694d;
}

.main:nth-child(5):hover {
    background: #01aee84d;
}

.main:nth-child(6):hover {
    background: #f1008d4d;
}

.compliance {
    transition: 0.7s;
}

.compliance:hover {
    background: #50e56a4d;
}

.compliance:nth-child(1):hover {
    background: #01aee84d;
}

.compliance:nth-child(2):hover {
    background: #d299024d;
}

.compliance:nth-child(3):hover {
    background: #f1008d4d;
}

.compliance:nth-child(4):hover {
    background: #a4cc694d;
}

.compliance:nth-child(5):hover {
    background: #01aee84d;
}

.compliance .services_inner .services_inner_txt {
    color: #fff;
}

.compliance .services_inner .services_inner_txt ul li {
    color: #fff;
}

.compliance .services_inner img {
    transition: 6s;
}

.compliance .services_inner:hover img {
    transform: scale(1.5);
}

